<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:background="@color/black"
        android:orientation="vertical"
        android:padding="5dp">

        <RelativeLayout
            android:id="@+id/relative_layout_type_one"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <com.nsquare.restaurant.util.RobotoRegular.CheckBoxTextRoboto
                android:id="@+id/checkbox_select_multiple_menu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="aaaaaaaaaa"
                android:visibility="visible"
                android:textSize="15sp"
                android:layout_below="@+id/textview_label"
                android:textColor="@color/grey_title"
                android:buttonTint="@color/grey_title"
                android:gravity="center" />

        </RelativeLayout>
    </LinearLayout>
</LinearLayout>