    <android.support.v4.widget.NestedScrollView
        android:id="@+id/scroll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/black"
        android:clipToPadding="false"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linearlayout_static_data"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="1">

                <com.nsquare.restaurant.util.RobotoBold.TextViewRoboto
                    android:id="@+id/activity_collapse_layout_textView_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:drawablePadding="8dp"
                    android:drawableRight="@drawable/heart_logo"
                    android:text=""
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="@color/white"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <RatingBar
                    style="@style/foodRatingBar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:isIndicator="true"
                    android:numStars="5"
                    android:progressDrawable="@drawable/custom_ratting_image"
                    android:rating="3"
                    android:stepSize="0.5"
                    android:visibility="gone"/>

                <com.nsquare.restaurant.util.RobotoLight.TextViewRoboto
                    android:id="@+id/activity_collapse_layout_textView_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="5dp"
                    android:drawableLeft="@drawable/address_logo_h"
                    android:drawablePadding="5dp"
                    android:text="Cyber Hub, Building No.2, DLF Phase-2, Gurgaon"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="@color/white"
                    android:textSize="15dp"
                    android:visibility="gone"/>

                <com.nsquare.restaurant.util.RobotoLight.TextViewRoboto
                    android:id="@+id/activity_collapse_layout_textView_mobile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="5dp"
                    android:drawableLeft="@drawable/call_logo_h"
                    android:drawablePadding="5dp"
                    android:text="01234-78945"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/white"
                    android:textSize="15dp"
                    android:visibility="gone"/>

                <com.nsquare.restaurant.util.RobotoLight.TextViewRoboto
                    android:id="@+id/activity_collapse_layout_textView_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="5dp"
                    android:drawableLeft="@drawable/price_logo_h"
                    android:drawablePadding="5dp"
                    android:text="1800 for two"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/white"
                    android:textSize="15dp"
                    android:visibility="gone"/>

                <com.nsquare.restaurant.util.RobotoRegular.ButtonTextRoboto
                    android:id="@+id/activity_collapse_layout_button_book_now"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/selector_button_send_booking_request"
                    android:text="@string/view_cart"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="18dp" />

                <com.nsquare.restaurant.util.RobotoRegular.ButtonTextRoboto
                    android:id="@+id/activity_collapse_layout_button_make_payment"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/selector_button_send_booking_request"
                    android:text="@string/make_payment"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="18dp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearlayout_viewpager"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <android.support.design.widget.AppBarLayout
                    android:id="@+id/activity_museum_appBarLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

                    <!--<android.support.v7.widget.Toolbar-->
                    <!--android:id="@+id/toolbar"-->
                    <!--android:layout_width="match_parent"-->
                    <!--android:layout_height="?attr/actionBarSize"-->
                    <!--android:background="@color/actionBar"-->
                    <!--app:layout_scrollFlags="scroll|enterAlways"-->
                    <!--app:popupTheme="@style/ThemeOverlay.AppCompat.Light"/>-->

                    <android.support.design.widget.TabLayout
                        android:id="@+id/tabs"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/black"
                        app:tabGravity="fill"
                        app:tabIndicatorColor="@color/orange_light"
                        app:tabMode="fixed" />

                </android.support.design.widget.AppBarLayout>


                <android.support.v4.view.ViewPager
                    android:id="@+id/viewpager"
                    android:layout_width="match_parent"
                    android:layout_height="1000dp"
                    android:layout_below="@id/activity_museum_appBarLayout"
                    app:layout_behavior="@string/appbar_scrolling_view_behavior" />

            </LinearLayout>


        </LinearLayout>
        <!--<include layout="@layout/content_layout" />-->

    </android.support.v4.widget.NestedScrollView>

    <android.support.design.widget.AppBarLayout
        android:id="@+id/appbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/black"
        android:fitsSystemWindows="true"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

        <android.support.design.widget.CollapsingToolbarLayout
            android:id="@+id/collapsing_toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:expandedTitleMarginBottom="32dp"
            app:expandedTitleMarginEnd="64dp"
            app:expandedTitleMarginStart="48dp"
            app:layout_scrollFlags="scroll|exitUntilCollapsed">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_collapseMode="parallax">

                <com.nsquare.restaurant.util.RobotoMedium.TextViewRoboto
                    android:id="@+id/activity_collapse_layout_textView_category_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:text="Pub Finder"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="@color/white"
                    android:textSize="16dp" />

                <com.nsquare.restaurant.gateway.wheelcarousel.WheelCarousel
                    android:id="@+id/wheelCarouselOuter"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <com.nsquare.restaurant.gateway.wheelcarousel.Carousel
                        android:id="@+id/wheelCarousel"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />

                    <com.nsquare.restaurant.gateway.wheelcarousel.WheelBackground
                        android:id="@+id/wheelBackground"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />

                    <com.nsquare.restaurant.gateway.wheelcarousel.WheelSections
                        android:id="@+id/wheelSections"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />

                    <RelativeLayout
                        android:id="@+id/loadingSpinner"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ProgressBar
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true" />
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/emptyContentMessage"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/black"
                        android:text=""
                        android:textColor="#fff"
                        android:visibility="gone" />
                </com.nsquare.restaurant.gateway.wheelcarousel.WheelCarousel>

            </LinearLayout>


